<?xml version="1.0" encoding="UTF-8"?>
<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    <mapper namespace="nankang.dao.mapper.IdentifyMapper">
    
    <resultMap id="identifyResultMap" type="nankang.dao.domain.Identify">
    <id column="id" property="id"></id>
    <result column="user_id" property="userId"></result>
    <result column="address" property="address"></result>
    </resultMap>
    
    <select parameterType="java.lang.Integer" resultMap="identifyResultMap" id="getIdentifyByUserId">
    	select * from identify where user_id=#{id}
    </select>
    </mapper>